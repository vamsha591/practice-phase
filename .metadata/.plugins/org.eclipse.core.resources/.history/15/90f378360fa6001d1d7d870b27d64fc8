server:
  port: 8084
spring:
  application:
    name: "API_GATEWAY"
  cloud:
    gateway:
      routes:
      - id: EMPLOYEE-SERVICE
        uri: lb://EMPLOYEE-SERVICE/
        predicates:
        - Path=/Employee/**  
  
 
